package edu.uga.cs1302.list;

public class Node<T>{
    private T data;
    private Node<T> next;
    private Node<T> prev;
    
    public Node(){
	next = null;
	prev = null;
	data = null;	
    }
    public Node(T data, Node next, Node prev){
	this.data = data;
	this.next = next;
	this.prev = prev;
	 
    }
    public Node getNext(){
	return next;
    }
    public Node getPrev(){
	return prev;
    }
    public T getData(){
	return data;
    }
    public void  setNext(Node next){
	this.next = next;
    }
    public void setPrev(Node prev){
	this.prev = prev;
    }
    public void setData(T data){
	this.data = data;
    }
    
}